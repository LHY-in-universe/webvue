{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api"
      },
      {
        "source": "/(.*)",
        "destination": "/frontend/$1"
      }
    ],
    "functions": {
      "backend/main.py": {
        "runtime": "now-php@1.0.0"
      }
    },
    "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/main.py"
    }
  ]
  }